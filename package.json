{
  "name": "d3-parliament-chart",
  "version": "0.0.0",
  "description": "parliament / congress / legislature charts simplified",
  "main": "build/d3-parliament-chart.cjs.js",
  "jsnext:main": "build/d3-parliament-chart.es.js",
  "files": [
    "build",
    "README.md"
  ],
  "scripts": {
    "dev": "rollup -c rollup.dev.config.js -w",
    "clean": "rimraf build",
    "lint": "eslint .",
    "test": "jest",
    "build": "npm run clean && npm run build:cjs && npm run build:es",
    "build:cjs": "rollup -c -o build/d3-parliament-chart.cjs.js -f cjs",
    "build:es": "rollup -c -o build/d3-parliament-chart.es.js -f es",
    "prepare": "npm run test && uglifyjs build/d3-foo.js -c -m -o build/d3-foo.min.js",
    "prepublishOnly": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dkaoster/d3-parliament-chart.git"
  },
  "author": "Daniel Kao",
  "license": "MIT",
  "keywords": [
    "d3",
    "d3-module",
    "parliament",
    "legislature",
    "congress",
    "chart"
  ],
  "bugs": {
    "url": "https://github.com/dkaoster/d3-parliament-chart/issues"
  },
  "homepage": "https://github.com/dkaoster/d3-parliament-chart#readme",
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "@rollup/plugin-commonjs": "^17.0.0",
    "@rollup/plugin-node-resolve": "^11.0.1",
    "babel-jest": "^26.6.3",
    "eslint": "^7.16.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-plugin-import": "^2.22.1",
    "jest": "^26.6.3",
    "rollup": "^2.36.1",
    "rollup-plugin-css-only": "^3.1.0",
    "rollup-plugin-livereload": "^2.0.0",
    "rollup-plugin-svelte": "^7.0.0",
    "sirv-cli": "^1.0.10",
    "svelte": "^3.31.2"
  }
}
